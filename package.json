{
  "name": "openlayers-demo",
  "scripts": {
    "dev": "npm run dev:database && concurrently npm:dev:client npm:dev:server",
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev",
    "dev:database": "docker compose create && docker compose start",
    "db:import": "npm run db:import:kommuner && npm run db:import:transform",
    "db:import:kommuner": "docker exec -i postgis /usr/bin/psql --user postgres < data/Basisdata_0000_Norge_25833_Kommuner_PostGIS.sql",
    "db:import:transform": "docker exec -i postgis /usr/bin/psql --user postgres < database/import.sql",
    "test": "tsc --noEmit && prettier --check .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
